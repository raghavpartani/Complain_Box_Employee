<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.complain_box">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Resolved_Complaints"></activity>
        <activity android:name=".Approved_Complaints" />
        <activity
            android:name=".MyComplaints"
            android:label="My Complaints"
            android:screenOrientation="portrait" />
        <activity android:name=".raise_a_compalint" />
        <activity android:name=".Complain_raised_by_companions" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUp" />
    </application>

</manifest>